{
    "name": "astrogoat/zero-ssl",
    "description": "A ZeroSsl app for Strata",
    "keywords": [
        "astrogoat",
        "laravel",
        "zero-ssl"
    ],
    "homepage": "https://github.com/astrogoat/zero-ssl",
    "license": "MIT",
    "authors": [
        {
            "name": "Laura Tonning",
            "email": "tonning@gmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.hlx.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "astrogoat/strata": "^0.7.0|^0.8.0|^0.9.0|^0.10.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37 || ^9.9|^10.0|^11.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3|^7.0|^8.0",
        "orchestra/testbench": "^6.15|^8.5",
        "pestphp/pest": "^1.18|^2.16|^3.0",
        "pestphp/pest-plugin-laravel": "^1.1|^2.2|^3.0",
        "vimeo/psalm": "^4.8|^5.15"
    },
    "autoload": {
        "psr-4": {
            "Astrogoat\\ZeroSsl\\": "src",
            "Astrogoat\\ZeroSsl\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrogoat\\ZeroSsl\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "vaimo/composer-patches": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astrogoat\\ZeroSsl\\ZeroSslServiceProvider"
            ],
            "aliases": {
                "ZeroSsl": "Astrogoat\\ZeroSsl\\ZeroSslFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
